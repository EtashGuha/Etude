{
  "name": "Etude",
  "version": "1.0.3",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Etude",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder -p always",
    "dist": "electron-builder"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^21.2.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.1.0",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.8.5"
  },
  "build": {
    "appId": "com.github.EtashGuha.Etude",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "EtashGuha",
        "repo": "Etude",
        "private": true,
        "token":"8aa9a02a6f29cdbfb608a3da1a799d0905c32949"
      }
    ],
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "asarUnpack" : [
      "MacKernel.jar",
      "node_modules/java",
      "node_modules/async",
      "WolframContents"
    ]
  },
  "dependencies": {
    "deepai": "^1.0.0",
    "electron-build": "0.0.3",
    "electron-log": "^3.0.6",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.1.2",
    "fs": "0.0.1-security",
    "html-to-json": "^0.6.0",
    "hummus": "^1.0.104",
    "java": "^0.11.1",
    "node-jre": "^0.2.3",
    "pdf.js-extract": "^0.1.1",
    "sentence-tokenizer": "^1.0.1",
    "string-similarity": "^3.0.0",
    "typeof": "^1.0.0"
  }
}
